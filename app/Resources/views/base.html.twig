<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Paltform Taxi Proyect{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{asset('css/bootstrap.min.css')}}" ></link>
            <link rel="stylesheet" href="{{asset('sweetalert2/dist/sweetalert2.min.css')}}">
            <link rel="stylesheet" href="{{asset('DataTables/datatables.min.css')}}" ></link>
            <link rel="stylesheet" href="{{asset('css/animate.css')}}" ></link>
            <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
            <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
            <link rel="stylesheet" href="{{asset('css/style.min.css')}}" ></link>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body style="min-height:100vh">
        {% block body %}
            {% block nav %}
                
            {% endblock %}

            {% block main %}
                <main></main>
            {% endblock %}
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('js/jquery-3.3.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script type="text/javascript" src="{{ asset('DataTables/datatables.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/main.js') }}"></script>
            <script src="{{ asset('node_modules/sweetalert2/dist/sweetalert2.all.min.js')}}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
